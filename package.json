{
  "name": "synapse-native",
  "version": "1.0.0",
  "private": true,
  "description": "A sophisticated word navigation puzzle game with semantic relationships and AI-powered challenges",
  "keywords": [
    "word game",
    "puzzle",
    "educational",
    "vocabulary",
    "brain training"
  ],
  "author": "Your Name <your.email@example.com>",
  "homepage": "https://synapsegame.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/synapse.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/synapse/issues"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.16.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint .",
    "test": "jest",
    "test:ci": "jest --coverage --watchAll=false --ci --testTimeout=30000 --forceExit",
    "build:web": "npx expo export:web && ./scripts/post-build.sh",
    "build:web-analyze": "EXPO_PUBLIC_ANALYZE_BUNDLE=true npx expo export:web && ./scripts/post-build.sh",
    "serve": "npx serve web-build -p 3000 -s",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update:production": "eas update --branch production",
    "analyze-performance": "ts-node scripts/analyzePerformance.ts",
    "analyze-bundle": "npx expo export -p web --source-maps && npx source-map-explorer web-build/static/js/*.js",
    "generate-challenges": "cd scripts && npx tsx generateThemedChallenges.ts",
    "generate-qr-cards": "cd scripts && npx tsx generateQRCards.ts",
    "generate-guerrilla-kit": "npm run generate-challenges && npm run generate-qr-cards",
    "prebuild": "expo prebuild --clean"
  },
  "dependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@expo/metro-runtime": "~3.1.3",
    "@expo/vector-icons": "^14.1.0",
    "@expo/webpack-config": "~19.0.1",
    "@hcaptcha/react-hcaptcha": "^1.12.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-masked-view/masked-view": "0.3.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^6.10.0",
    "@react-navigation/stack": "^7.3.2",
    "@stripe/stripe-js": "^7.3.1",
    "@supabase/supabase-js": "^2.50.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-expo": "^10.0.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
